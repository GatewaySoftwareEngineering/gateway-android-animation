<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BannerView">
        <attr name="displayImageDuration" format="integer" />
        <attr name="nextImageDelay" format="integer" />
        <attr name="fadeInDuration" format="integer" />
        <attr name="fadeOutDuration" format="integer" />
        <attr name="foreground" format="reference|color" />
        <attr name="scaleType">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
    </declare-styleable>
</resources>